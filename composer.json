{
  "name": "spryker/suite-nonsplit",
  "description": "Spryker Suite (internal)",
  "license": "MIT",
  "require": {
    "spryker/spryker": "dev-feature/glue-9871/glue-9039-e2e-tests-for-checkout",
    "spryker/spryker-shop": "dev-feature/glue-9871/dev-e2e-tests-for-critical-endpoints",
    "spryker/decimal-object": "^1.0.0",
    "spryker-eco/loggly": "dev-master",
    "spryker/rabbit-mq": "dev-master",
    "spryker/install": "dev-master",
    "spryker/event-behavior": "dev-master as 1.15.0",
    "spryker/synchronization-behavior": "dev-master as 1.4.0",
    "spryker/uuid-behavior": "dev-master as 1.1.0",
    "spryker/silexphp": "dev-master as 0.3.1",
    "php": ">=7.2",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-gmp": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-pdo_pgsql": "*",
    "ext-pgsql": "*",
    "ext-readline": "*",
    "ext-redis": "*"
  },
  "require-dev": {
    "spryker/architecture-sniffer": "dev-master",
    "spryker/development-core": "dev-master",
    "spryker/code-generator": "dev-master",
    "spryker/code-sniffer": "dev-master",
    "spryker/git-hook": "dev-master",
    "galbar/jsonpath": "^1.0.0",
    "justinrainbow/json-schema": "^5.2.0",
    "pdepend/pdepend": "^2.2.2",
    "phpmd/phpmd": "~2.7.0",
    "ocramius/package-versions": "~1.4.0",
    "sebastian/phpcpd": "^4.0.0",
    "sensiolabs/security-checker": "*",
    "symfony/routing": "<4.4.0",
    "symfony/web-profiler-bundle": "^4.0.0",
    "symfony/var-dumper": "*",
    "spryker-sdk/codeception-phantoman": "dev-master",
    "sllh/composer-versions-check": "^2.0.2",
    "jakoch/phantomjs-installer": "^2.1.0",
    "stecman/symfony-console-completion": "*",
    "fzaninotto/faker": "^1.6.0",
    "ajgl/breakpoint-twig-extension": "^0.3.1",
    "spryker-sdk/phpstan-spryker": "dev-master",
    "spryker-sdk/composer-constrainer": "dev-master",
    "spryker-sdk/composer-replace": "dev-master",
    "spryker-sdk/spryk": "dev-master as 0.2.2",
    "spryker-sdk/spryk-gui": "dev-master",
    "dms/phpunit-arraysubset-asserts": "^0.1.0",
    "wikimedia/composer-merge-plugin": "^1.4.1",
    "codeception/module-rest": "^1.0",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-asserts": "^1.1",
    "codeception/module-webdriver": "^1.0",
    "codeception/module-cli": "^1.0",
    "codeception/module-filesystem": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Generated\\": "src/Generated/",
      "Orm\\": "src/Orm/",
      "Pyz\\": "src/Pyz/",
      "Spryker\\": "vendor/spryker/spryker/Bundles/",
      "SprykerShop\\": "vendor/spryker/spryker-shop/Bundles/"
    },
    "psr-0": {
      "Pimple": "vendor/spryker/spryker/Bundles/Container/src/Spryker"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PyzTest\\": "tests/PyzTest/"
    },
    "files": [
      "test-autoload.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.2.15"
    },
    "use-include-path": true,
    "process-timeout": 900
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker-shop.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/development-core.git"
    }
  ],
  "scripts": {
    "compile": [
      "mkdir -p ./data/DE/logs/ZED"
    ],
    "post-install-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
    ],
    "post-update-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
    ],
    "phpstan": [
      "php -d memory_limit=1024M vendor/bin/phpstan analyze -l 5 -c phpstan.neon src/"
    ],
    "phpstan-setup": "vendor/bin/console dev:ide:generate-auto-completion"
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "vendor/spryker/spryker/Bundles/*/composer.json",
        "vendor/spryker/spryker-shop/Bundles/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": false
    }
  }
}
