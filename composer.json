    "spryker/silexphp": "dev-master as 0.3.2",
    "spryker-eco/loggly": "dev-master",
    "spryker-eco/new-relic": "dev-master",
    "spryker-eco/payone": "dev-master as 4.0.2",
    "spryker/decimal-object": "^1.0.0",
    "php": ">=7.2",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-gmp": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-pdo_pgsql": "*",
    "ext-pgsql": "*",
    "ext-readline": "*",
    "ext-redis": "*",
    "codeitnowin/barcode": "^3.0.0",
    "doctrine/inflector": "~1.3.0",
    "egulias/email-validator": "~2.1.2",
    "everon/collection": "^1.0.0",
    "everon/criteria-builder": "~1.1.6",
    "everon/factory": "~1.1.4",
    "everon/utils": "~1.0.4",
    "filp/whoops": "^2.4",
    "galbar/jsonpath": "^1.0",
    "guzzlehttp/guzzle": "^6.3.0 || ^7.0.0",
    "justinrainbow/json-schema": "^5.2",
    "league/csv": "^9.2",
    "league/flysystem": "^1.0.8",
    "league/flysystem-aws-s3-v3": "^1.0.1",
    "league/oauth2-server": "^8.0.0",
    "mikey179/vfsstream": "^1.6",
    "moneyphp/money": "^3.0.0",
    "monolog/monolog": "^1.25.0 || ^2.0.0",
    "nette/di": "^2.4.7 || ^3.0",
    "phpdocumentor/graphviz": "^1.0.4",
    "predis/predis": "^1.1.1",
    "propel/propel": "2.0.0-alpha11",
    "psr/container": "^1.0.0",
    "psr/log": "^1.0.0",
    "ramsey/uuid": "^4.0.1",
    "ruflin/elastica": "^6.0.0 || ^7.0.0",
    "swiftmailer/swiftmailer": "^5.4.5 || ^6.1",
    "symfony-cmf/routing": "^1.3.0 || ^2.0.0",
    "symfony/console": "^4.4.0",
    "symfony/debug": "^4.0.0",
    "symfony/filesystem": "^4.1.0",
    "symfony/finder": "^4.0.0",
    "symfony/form": "^4.3.0",
    "symfony/http-kernel": "^4.3.0",
    "symfony/http-foundation": "^4.2.0",
    "symfony/intl": "^3.0.0 || ^4.0.0",
    "symfony/mime": "^4.0.0 || ^5.0.0",
    "symfony/options-resolver": "^3.0.0 || ^4.0.0",
    "symfony/process": "^4.2.0",
    "symfony/property-access": "^3.0.0 || ^4.0.0",
    "symfony/routing": "^4.3.0",
    "symfony/security": "^4.1.12 <4.2.0 || ^4.2.7 < 4.4.0 || ^4.4.7",
    "symfony/serializer": "^3.0.0 || ^4.0.0",
    "symfony/stopwatch": "^3.0.0 || ^4.0.0",
    "symfony/translation": "^4.2.0",
    "symfony/twig-bridge": "^3.4.0 || ^4.0.0",
    "symfony/validator": "^4.3.0",
    "symfony/web-profiler-bundle": "^4.0.0",
    "symfony/yaml": "^4.0.0",
    "twig/twig": "^1.41.0 || ^2.9.0 || ^3.0.0",
    "voku/anti-xss": "^4.1.20",
    "webmozart/glob": "^4.0.0",
    "willdurand/negotiation": "^2.3",
    "zendframework/zend-config": "^2.5.1 || ^3.1.0",
    "zendframework/zend-filter": "^2.5.1",
    "zendframework/zend-servicemanager": "^2.7.4 || ^3.3.0"
  },
  "require-dev": {
    "codeception/codeception": "dev-feature/deactivate-c3-call-in-localserver as 4.1.7",
    "phpunit/phpunit": "^8.5.8",
    "codeception/lib-innerbrowser": "dev-feature/use-client-through-getter as 1.3.2",
    "spryker/architecture-sniffer": "dev-master",
    "spryker/development-core": "dev-master",
    "spryker/code-generator": "dev-master",
    "spryker/code-sniffer": "dev-master",
    "spryker/git-hook": "dev-master",
    "pdepend/pdepend": "^2.2.2",
    "composer/package-versions-deprecated": "^1.8.0",
    "sebastian/phpcpd": "^4.0.0",
    "sensiolabs/security-checker": "*",
    "symfony/var-dumper": "*",
    "spryker-sdk/codeception-phantoman": "dev-master",
    "sllh/composer-versions-check": "^2.0.2",
    "jakoch/phantomjs-installer": " ^3.0.0",
    "stecman/symfony-console-completion": "*",
    "fzaninotto/faker": "^1.6.0",
    "spryker-sdk/phpstan-spryker": "^0.3.0",
    "spryker-sdk/composer-constrainer": "dev-master",
    "spryker-sdk/composer-replace": "dev-master",
    "spryker-sdk/spryk": "dev-master as 0.2.7",
    "spryker-sdk/spryk-gui": "dev-master",
    "dms/phpunit-arraysubset-asserts": "^0.1.0",
    "codeception/phpunit-wrapper": "^8.1.2",
    "codeception/lib-asserts": "^1.12.0",
    "codeception/module-rest": "^1.0",
    "codeception/module-phpbrowser": "^1.0.1",
    "codeception/module-asserts": "^1.2.1",
    "codeception/module-webdriver": "^1.1.0",
    "codeception/module-cli": "^1.0.2",
    "codeception/module-filesystem": "^1.0.2",
    "codeception/stub": "^3.7.0",
    "psalm/phar": "~3.14.2",
    "sebastian/git": "^2.1",
    "sebastian/diff": "^3.0",
    "phpstan/phpstan": "0.12.25",
    "nette/neon": "^3.2.1",
    "roave/better-reflection": "~4.3.0",
    "spryker-sdk/benchmark": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Generated\\": "src/Generated/",
      "Orm\\": "src/Orm/",
      "Pyz\\": "src/Pyz/",
      "SprykerConfig\\": "src/SprykerConfig"
    },
    "psr-0": {
      "Pimple": "vendor/spryker/spryker/Bundles/Container/src/Spryker"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PyzTest\\": "tests/PyzTest/",
      "Benchmark\\": "tests/Benchmark/"
    },
    "files": [
      "test-autoload.php"
    ],
    "exclude-from-classmap": [
      "/tests/",
      "vendor/spryker/spryker/Bundles/*/tests/",
      "vendor/spryker/spryker-shop/Bundles/*/tests/",
      "vendor/flow/jsonpath/tests/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.2.15"
    },
    "use-include-path": true,
    "process-timeout": 900
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker-sdk/lib-innerbrowser.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/Codeception.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker-shop.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/development-core.git"
    }
  ],
  "scripts": {
    "compile": [
      "mkdir -p ./data/DE/logs/ZED"
    ],
    "post-install-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
    ],
    "post-update-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
    ],
    "phpstan": [
      "php -d memory_limit=1024M vendor/bin/phpstan analyze -l 5 -c phpstan.neon src/"
    ],
    "phpstan-setup": "vendor/bin/console dev:ide-auto-completion:generate",
    "psalm": "vendor/bin/psalm.phar --show-info=false",
    "psalm-update-baseline": "vendor/bin/psalm.phar --update-baseline",
    "psalm-set-baseline": "vendor/bin/psalm.phar --set-baseline=psalm-baseline.xml",
    "psalm-update-report": "vendor/bin/psalm.phar --report=psalm-report.xml",
    "prefer-mid": [
      "COMPOSER_MEMORY_LIMIT=-1 composer require twig/twig:\"^2.9.0\""
    ]
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "vendor/spryker/spryker/Bundles/*/composer.json",
        "vendor/spryker/spryker-shop/Bundles/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": false
    }
  }
}
