{
  "name": "spryker/suite-nonsplit",
  "description": "Spryker Suite (internal)",
  "license": "MIT",
  "require": {
    "spryker/spryker": "dev-feature/te-963/dev-create-form-application-extension",
    "spryker/spryker-shop": "dev-feature/te-963/dev-create-form-application-extension",
    "spryker-eco/loggly": "dev-master",
    "spryker/rabbit-mq": "dev-master",
    "spryker/install": "dev-master",
    "spryker/event-behavior": "dev-master",
    "spryker/synchronization-behavior": "dev-master",
    "spryker/uuid-behavior": "dev-master",
    "spryker/silexphp": "dev-feature/te-963/dev-create-form-application-extension",
    "php": ">=7.1",
    "codeitnowin/barcode": "^3.0.0",
    "guzzlehttp/guzzle": "~6.3.0",
    "league/csv": "^9.1",
    "filp/whoops": "^2.1",
    "mandrill/mandrill": "~1.0.55",
    "monolog/monolog": "^1.21.0",
    "predis/predis": "^1.1.1",
    "propel/propel": "2.0.0-alpha8",
    "psr/log": "^1.0.0",
    "ruflin/elastica": "~5.3.0",
    "symfony-cmf/routing": "^2.0.0",
    "symfony/console": "^4.0.0",
    "symfony/debug": "^4.0.0",
    "symfony/filesystem": "^4.0.0",
    "symfony/finder": "^4.0.0",
    "symfony/form": "^4.0.0",
    "symfony/http-kernel": "^4.0.0",
    "symfony/intl": "^4.0.0",
    "symfony/options-resolver": "^4.0.0",
    "symfony/process": "^4.0.0",
    "symfony/property-access": "^4.0.0",
    "symfony/routing": "^4.0.0",
    "symfony/security": "^4.0.0",
    "symfony/serializer": "^4.0.0",
    "symfony/translation": "^4.0.0",
    "symfony/twig-bridge": "^4.0.0",
    "symfony/validator": "^4.0.0",
    "symfony/yaml": "^4.0.0",
    "swiftmailer/swiftmailer": "^6.1",
    "twig/twig": "^1.20.0",
    "zendframework/zend-config": "^3.1.0",
    "zendframework/zend-filter": "^2.5.1",
    "zendframework/zend-servicemanager": "^3.3",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-gmp": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo_pgsql": "*",
    "ext-pgsql": "*",
    "ext-readline": "*",
    "ext-redis": "*",
    "league/flysystem": "^1.0",
    "egulias/email-validator": "^2.1.2",
    "league/oauth2-server": "^7.1",
    "willdurand/negotiation": "^2.3",
    "ramsey/uuid": "^3.8.0",
    "phpdocumentor/reflection-docblock": "^4.3",
    "doctrine/inflector": "^1.3"
  },
  "require-dev": {
    "spryker/architecture-sniffer": "dev-master",
    "spryker/development-core": "dev-master",
    "spryker/code-generator": "dev-master",
    "spryker/code-sniffer": "dev-master",
    "spryker/git-hook": "dev-master",
    "codeception/codeception": "^3.0.0",
    "doctrine/lexer": "~1.0.2",
    "galbar/jsonpath": "^1.0",
    "justinrainbow/json-schema": "^5.2",
    "pdepend/pdepend": "^2.2.2",
    "phpmd/phpmd": "^2.4",
    "sebastian/phpcpd": "^4.0.0",
    "sensiolabs/security-checker": "*",
    "symfony/stopwatch": "^4.0.0",
    "symfony/web-profiler-bundle": "^4.2.9",
    "symfony/var-dumper": "*",
    "spryker-sdk/codeception-phantoman": "dev-master",
    "sllh/composer-versions-check": "^2.0.2",
    "jakoch/phantomjs-installer": "^2.1.0",
    "stecman/symfony-console-completion": "*",
    "fzaninotto/faker": "^1.6",
    "ajgl/breakpoint-twig-extension": "^0.3.1",
    "spryker-sdk/phpstan-spryker": "dev-master",
    "spryker-sdk/composer-constrainer": "dev-master",
    "spryker-sdk/spryk": "dev-master as 0.2.2",
    "spryker-sdk/spryk-gui": "dev-master",
    "roave/better-reflection": "^3.3.0",
    "ocramius/package-versions": "~1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Generated\\": "src/Generated/",
      "Orm\\": "src/Orm/",
      "Pyz\\": "src/Pyz/",
      "Spryker\\": "vendor/spryker/spryker/Bundles/",
      "SprykerShop\\": "vendor/spryker/spryker-shop/Bundles/",
      "SprykerMigrator\\": "vendor/spryker-sdk/migrator/SprykerShop/"
    },
    "psr-0": {
      "Pimple": "vendor/spryker/spryker/Bundles/Container/src/Spryker"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PyzTest\\": "tests/PyzTest/"
    },
    "files": [
      "test-autoload.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.1.11"
    },
    "use-include-path": true,
    "process-timeout": 900
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker-shop.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/development-core.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker-sdk/composer-constrainer.git"
    }
  ],
  "scripts": {
    "compile": [
      "mkdir -p ./data/DE/logs/ZED"
    ],
    "post-install-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
    ],
    "post-update-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
      "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
    ],
    "phpstan": [
      "php -d memory_limit=1024M vendor/bin/phpstan analyze -l 5 -c phpstan.neon src/"
    ],
    "phpstan-setup": "vendor/bin/console dev:ide:generate-auto-completion"
  }
}
